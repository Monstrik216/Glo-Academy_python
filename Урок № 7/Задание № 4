print('Введите 2 числа:')
a = int(input())
b = int(input())
if a == b:
        if a % 2 == 0:
                print(a)
        elif a > b:
                for i in range(a, b + 1):
                        if i % 2 == 0:
                                print(i)
else:
        for i in range(b, a + 1):
                if i % 2 == 0:
                        print(i)
